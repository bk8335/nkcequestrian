<%= form_for(@post) do |f| %>
	<%= render "shared/error_messages" %>
	<%= f.label :title %><br>
	<%= f.text_field :title, placeholder: "Type the blog title here" %><br>
	<%= f.label :content %><br>
	<%= f.text_area :content, size: “60x12”, placeholder: "Type the blog content here" %><br>
	<%= f.submit %>
<% end %>